        "npm-run-all": "^4.1.5",
        "patch-package": "6.4.7",
        "postinstall-postinstall": "2.1.0",
        "prettier": "^2.3.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "^5.0.1",
        "ts-jest": "^28.0.8",
        "typescript": "^4.2.3"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
